{
  "name": "multi-verse",
  "version": "2.2.0",
  "description": "polymer elements for multivariate analysis (depend on crossfilter, universe, reductio, multichart and multiverse)",
  "license": "MIT",
  "author": {
    "name": "Christophe Geiser"
  },
  "authors": [
    "christophe.geiser <christophe.geiser@e-smile.org>"
  ],
  "homepage": "webcomponents.org/polymerEl/multi-verse",
  "main": "multi-verse-import.html",
  "repository": "https://github.com/polymerEl/multi-verse",
  "keywords": [
    "web-component",
    "web-components",
    "d3.js",
    "charts",
    "multivariate",
    "dashboard",
    "polymer",
    "crossfilter",
    "universe",
    "Polymer",
    "analytics",
    "mapping",
    "dataviz"
  ],
  "dependencies": {
    "universe": "git://github.com/PolymerEl/universe.git",
    "crossfilter2": "^1.4.6",
    "reductio": "git://github.com/PolymerEl/reductio.git#strict-mode"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babelify": "^7.3.0",
    "json2module": "0.0",
    "package-preamble": "0.0",
    "rimraf": "2",
    "rollup": "0.66",
    "rollup-plugin-ascii": "0.0",
    "rollup-plugin-commonjs": "*",
    "rollup-plugin-json": "*",
    "rollup-plugin-license": "^0.7.0",
    "rollup-plugin-node-resolve": "3",
    "rollup-plugin-terser": "1"
  },
  "scripts": {
    "build-universe": "rollup -c rollup.universe.config.js",
    "pre-build": "rimraf build && mkdir build &&  json2module package.json > build/package.js ",
    "prepublish": "npm run pre-build  && npm run build-universe",
    "postpublish": "git push && git push --tags"
  }
}
